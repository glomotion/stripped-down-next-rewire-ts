{
  "name": "imx-link-next",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bootstrap": "npm i; npm dedup --legacy-peer-deps",
    "dev": "next dev",
    "build": "next build",
    "analyze:build": "ANALYZE=true next build",
    "start": "next start",
    "plop": "plop",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pretest": "npm run lint",
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint:quiet": "eslint --ext .ts,.tsx,.js,.jsx --quiet .",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --fix .",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:jest:run; npm run test:cy:run",
    "test:jest": "npm run test:jest:run",
    "test:jest:run": "cross-env NODE_ENV=test jest",
    "test:jest:coverage": "cross-env NODE_ENV=test npm run test:jest:run -- --coverage",
    "test:jest:watch": "npm run test:jest:run -- --watch",
    "test:cy": "npm run test:cy:run",
    "test:cy:run": "cross-env NODE_ENV=test cypress run-ct",
    "test:cy:coverage": "cross-env NODE_ENV=test cypress run-ct --env coverage=true",
    "test:cy:watch": "cross-env NODE_ENV=test cypress open-ct --browser electron",
    "test:coverage": "npm run test:jest:coverage && npm run test:cy:coverage && npm run test:coverage:copy:reports && npm run test:coverage:report",
    "test:coverage:copy:reports": "mkdir reports; cp coverage/cypress/coverage-final.json reports/from-cypress.json && cp coverage/jest/coverage-final.json reports/from-jest.json",
    "test:coverage:report": "mkdir -p .nyc_output && npx nyc merge reports .nyc_output/out.json && npx nyc report",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@imtbl/design-system": "^3.7.0",
    "@next/bundle-analyzer": "^10.2.3",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/react": "^5.10.2",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-rewire-exports": "^2.3.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "cypress": "^8.7.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "https-browserify": "^1.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "local-cypress": "^1.2.1",
    "plop": "^2.7.4",
    "prettier": "^2.3.0",
    "read-pkg-up": "^7.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "reporter": [
      "json"
    ]
  }
}
